@using Metanit_11_06_CreateBinder.Models

@{
    ViewData["Title"] = "Home Page";
}

<ul>
    <li> <a href="/Home/Create"> Создание события </a></li>
</ul>

<table class="table">
    @foreach (Event e in Model)
    {
        <tr>
            <td>@e.Id</td>
            <td>@e.Name</td>
            <td>@e.EventDate</td>
        </tr>
    }
</table>